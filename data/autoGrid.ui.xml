			<switchTextDisplay />
			<switchLed />
			<switch label="&amp;Menu" />
		</switchAssembly>
		<switchAssembly number="23" row="1" column="3" >
			<switchLed />
		</switchAssembly>
		<switchAssembly number="21" row="1" column="4" >
			<switchTextDisplay />
			<switchLed />
