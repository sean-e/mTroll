<?xml version="1.0" ?>
<MidiControlUiSettings autogrid="1" >
	<!-- layout grid coordinates are 1-based -->
	<frame width="1280" height="790" backgroundColor="1a1a1a" frameHighlightColor="333333" />
	<mainTextDisplay font-name="Lucida Console" font-height="44" foregroundColor="ffcc00" backgroundColor="0" row="5" column="1" columnSpan="3" />
	<traceTextDisplay font-height="8" row="5" column="4" columnSpan="2" />
	<hardware ledBrightness="1" />

	<switchAssemblyConfig>
		<switch font-name="Tahoma" font-weight="bold" font-height="13" foregroundColor="bbbbbb" />
		<switchTextDisplay font-name="Tahoma" font-height="24" foregroundColor="00ff00" backgroundColor="0" />
		<!-- do switchLed and switch need width and height?? -->
		<switchLed width="30" height="15" onColor="ffff00" offColor="3a3a3a" />
	</switchAssemblyConfig>

	<!-- this is for mapping ordinal switch numbers to monome grid coords (all are zero based) -->
	<switchMappings>
		<switchMap number="0" row="0" col="0" />
		<switchMap number="1" row="0" col="1" />
		<switchMap number="2" row="0" col="2" />
		<switchMap number="3" row="0" col="3" />
		<switchMap number="4" row="0" col="4" />

		<switchMap number="5" row="1" col="0" />
		<switchMap number="6" row="1" col="1" />
		<switchMap number="7" row="1" col="2" />
		<switchMap number="8" row="1" col="3" />
		<switchMap number="9" row="1" col="4" />

		<switchMap number="10" row="2" col="0" />
		<switchMap number="11" row="2" col="1" />
		<switchMap number="12" row="2" col="2" />
		<switchMap number="13" row="2" col="3" />
		<switchMap number="14" row="2" col="4" />

		<switchMap number="15" row="4" col="0" />
		<switchMap number="16" row="4" col="1" />
		<switchMap number="17" row="4" col="2" />
		<switchMap number="18" row="4" col="3" />
		<switchMap number="19" row="4" col="4" />

		<switchMap number="20" row="3" col="0" />
		<switchMap number="21" row="3" col="3" />
		<switchMap number="22" row="3" col="4" />
		<switchMap number="23" row="3" col="5" />
	</switchMappings>

	<switchAssemblies>
